{
  "name": "bestia-behemoth-web_alpha",
  "description": "Frontend of Bestia - Browsergame.",
  "author": "Thomas Felix (http://www.tfelix.de)",
  "version": "0.4.3",
  "jsnext:main": "build/bundle.js",
  "private": true,
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "clean": "rimraf build/*",
    "mkdir": "mkdirp ./build/css ./build/js",
    "server": "lite-server --baseDir=\"build\"",
    "build:js": "rollup --config rollup.config.js",
    "prebuild:watch": "npm run mkdir",
    "build:watch": "parallelshell \"npm run build:html -- -w\" \"npm run build:css:watch\" \"npm run build:js -- --watch\" \"npm run server\""
  },
  "dependencies": {
    "loglevel": "1.4.1",
    "phaser": "2.6.2",
    "phaser-ce": "2.8.8",
    "zepto": "1.2.0"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "cpx": "1.5.0",
    "cssmin": "0.4.3",
    "eslint": "3.16.1",
    "less": "2.7.2",
    "lite-server": "2.3.0",
    "mkdirp": "0.5.1",
    "mocha": "3.2.0",
    "parallelshell": "2.0.0",
    "rimraf": "2.6.1",
    "rollup": "0.49.2",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.0",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-watch": "4.3.1",
    "should": "11.2.1",
    "watch": "1.0.2"
  }
}
